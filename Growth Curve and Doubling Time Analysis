stats.GCraw <- GC.data %>% 
  group_by(tx, hour) %>%
  summarise(
    means = mean(estimate),
    stderr = sd(estimate)/sqrt(n())) %>%
  mutate(
    lwr.err = means - stderr,
    upr.err = means + stderr)

ggplot(stats.GCraw, aes(x = hour, fill = tx), show.legend = F) +
  theme_bw() +
  geom_bar(aes(y = means, fill = tx), stat='identity', width = 12, position = position_dodge(width = 15), show.legend = T) +
