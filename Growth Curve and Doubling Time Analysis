PD.raw <- GC.data.PDL
stats.PDraw <- PD.raw %>% 
  group_by(tx, hour) %>%
  summarise(
    means = mean(PDL),
    stderr = sd(PDL)/sqrt(n())) %>%
  mutate(
    lwr.err = means - stderr,
    upr.err = means + stderr)

